{
  "name": "servico-web-rest",
  "version": "1.0.0",
  "description": "Projeto de base para backend em NodeJs, Express e MongoDB",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "build": "babel ./src -d dist",
    "start": "npm install --progress=false && npm run build && export NODE_ENV=production && node dist/index.js",
    "dev": "npm install --progress=false && npm run build && export NODE_ENV=development && node dist/index.js",
    "serve": "nodemon --exec babel-node --experimental-json-modules ./src/index.js",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "author": "Felipe Fernandes e Vitor Ten√≥rio",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.0",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-paginate-v2": "^1.6.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.5",
    "nodemailer-express-handlebars": "^5.0.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/node": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.18.2",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.16",
    "sonar-scanner": "^3.1.0"
  }
}
